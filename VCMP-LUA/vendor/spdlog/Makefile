ifndef config
  config=release32
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild prelink

ifeq ($(config),release32)
  RESCOMP = windres
  TARGETDIR = ../../../bin/Release32-windows-x86/spdlog
  TARGET = $(TARGETDIR)/spdlog_x86.lib
  OBJDIR = ../../../bin/interm/Release32-windows-x86/spdlog
